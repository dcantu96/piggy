<div class="card">
  <div class="card-header">
    <h2><%= "#{@profile.first_name} #{@profile.last_name}" %></h2>
  </div>
  <div class="card-body">
    <h5 class="card-title">Email: <%= @profile.email %></h5>
    <p class="card-text">Username: <%= @profile.username %></p>
    <%= render 'layouts/dashboard/profile_button' %>
  </div>
</div>